{
  "kind": "singleType",
  "collectionName": "website_configs",
  "info": {
    "name": "Website Configs",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "keywords": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "theme": {
      "type": "enumeration",
      "enum": [
        "light",
        "dark",
        "cupcake",
        "bumblebee",
        "Emerald",
        "Corporate",
        "synthwave",
        "retro",
        "cyberpunk",
        "valentine"
      ],
      "required": true,
      "default": "cupcake"
    },
    "sidebar_logo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "title": {
      "type": "string",
      "required": true,
      "default": "Raditya Yudistira Yudiana"
    },
    "url": {
      "type": "string",
      "regex": "/[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)/gi"
    }
  }
}
