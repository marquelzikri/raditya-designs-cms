{
  "kind": "singleType",
  "collectionName": "social_media_configs",
  "info": {
    "name": "Social Media Config"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "twitter": {
      "type": "string",
      "regex": "(?:^|[^\\w])(?:@)([A-Za-z0-9_](?:(?:[A-Za-z0-9_]|(?:\\.(?!\\.))){0,28}(?:[A-Za-z0-9_]))?)",
      "required": true
    },
    "instagram": {
      "type": "string",
      "regex": "(?:^|[^\\w])(?:@)([A-Za-z0-9_](?:(?:[A-Za-z0-9_]|(?:\\.(?!\\.))){0,28}(?:[A-Za-z0-9_]))?)"
    },
    "linkedin": {
      "type": "string"
    }
  }
}
